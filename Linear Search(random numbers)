#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main() {
    srand(time(NULL));

    int numbers[10];
    printf("Generated 10 random numbers:\n");
    int i;
    for (i = 0; i < 10; i++) {
        numbers[i] = rand() % 100 + 1; 
        printf("%d ", numbers[i]);
    }
    printf("\n");

    int target;
    printf("Enter the number you want to search for: ");
    scanf("%d", &target);

    int found = 0;
    for (i = 0; i < 10; i++) {
        if (numbers[i] == target) {
            printf("Number %d found at index %d.\n", target, i);
            found = 1;
            break;
        }
    }
    
    if (found==0) {
        printf("Number %d not found in the array.\n", target);
    }
    return 0;
}
